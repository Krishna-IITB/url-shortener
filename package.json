    "ipapi.co": "^0.3.0",
    "morgan": "^1.10.1",
    "nanoid": "^5.1.6",
    "node-cron": "^4.2.1",
    "node-pg-migrate": "^7.9.1",
    "pg": "^8.16.3",
    "pg-types": "^4.1.0",
    "qrcode": "^1.5.4",
    "react-router-dom": "^7.9.6",
    "redis": "^5.10.0",
    "ua-parser-js": "^2.0.6",
    "useragent": "^2.3.0",
    "validator": "^13.15.23"
  },
